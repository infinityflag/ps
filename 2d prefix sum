#include <iostream>
#include <algorithm>
#include <iomanip>
#include <queue>
#include <stack>
#include <set>
#include <map>
#include <cstdlib>
#include <cstdio>
#include <cmath>
#include <math.h>
#include <vector>
#include <stdlib.h>
#include <cstring>
#include <string>
using namespace std;
typedef long long ll;
int arr[305][305];
int psa[501][501];
int main()
{
    int n,m;
    cin >> n >> m;
    for(int i=0;i<n;i++){
        for(int j=0;j<m;j++){
            cin >> arr[i][j];
        }
    }
    psa[0][0] = arr[0][0];
    for(int i=1;i<n;i++){
        psa[i][0] = psa[i-1][0] + arr[i][0];
    }
    for(int j=1;j<m;j++){
        psa[0][j] = psa[0][j-1] + arr[0][j];
    }
    int k;
    cin >> k;
    for(int i=0;i<k;i++){
        int a,b,c,d;
        cin >>a>>b>>c>>d;
        cout << psa[c-1][d-1]<<'\n';
        
        for(int i=a-1;i<c;i++){
            for(int j=b-1;j<d;j++){
                if(i == c-1 && j == d-1){
                    cout << psa[i][j] <<'\n';
                }
                psa[i][j] = psa[i-1][j] + psa[i][j-1] - psa[i-1][j-1] + arr[i-1][j-1];
            }
        }
    }
}
