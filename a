#include<iostream>
#include<deque>
#include<vector>
#include<math.h>
#include<cmath>
typedef long long ll;
using namespace std;
long long lim = 1e8;
int cnt[5*100000];
int a[5000][5000];
//int cnt = 0;
void solve(int x, int y, int k, int cnt){
    if(k == 1){
        cnt++;
        //cout << a[x][y];
        return;
    }
    else{
        for(int i=0;i<x+k;i++){
            for(int j=0;j<y+k;j++){
                if(a[x][y] != a[i][j]){
                    if(a[i][j] == -1){
                        
                    }
                    if(a[i][j] == 0){
                        
                    }
                    if(a[i][j] == 1){
                        
                    }
                    solve(x,y,k/3);
                    return;
                }
            }
        }
        //cout << a[x][y];
        cnt++;
    }
}
int main(){
    int n;
    cin >> n;
    for(int i=0;i<n;i++){
        for(int j=0;j<n;j++){
            cin >> a[i][j];
        }
    }
    cout << solve(0,0,n);

}
