#include <iostream>
#include <algorithm>
#include <iomanip>
#include <queue>
#include <stack>
#include <set>
#include <map>
#include <cstdlib>
#include <cstdio>
#include <cmath>
#include <math.h>
#include <vector>
#include <stdlib.h>
#include <cstring>
#include <string>
using namespace std;
typedef long long ll;
int arr[305][305];
int psa[501][501];
int main()
{
    string s;
    while(getline(cin,s)){
        if(s == "."){
            return 0;
        }
        int score = 0;
        for(char c: s){
            if(c == '('){
                score += 1;
            }
            if(c == ')'){
                score -= 1;
            }
            if(c == '['){
                score += 2;
            }
            if(c == ']'){
                score -= 2;
            }
        }
        if(score == 0){
            cout << "yes" <<'\n';
        }
        else{
            cout << "no" <<'\n';
        }
    }
}
