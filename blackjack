#include <iostream>
#include <algorithm>
#include <iomanip>
#include <queue>
#include <stack>
#include <set>
#include <map>
#include <cstdlib>
#include <cstdio>
#include <vector>
#include <stdlib.h>
#include <cstring>
#include <string>
#include <cmath>
using namespace std;
#define maxn 21
typedef long long ll;
//int arr[1000001];
int main()
{   
    int n,k;
    cin >> n >> k;
    vector<int>a(n);
    for(int i=0;i<n;i++){
        cin >> a[i];
    }
    int maxx = -1e9;
    for(int i=0;i<n;i++){
        for(int j=0;j<n;j++){
            for(int k=0;k<n;k++){
                int ch = a[i]+a[j]+a[k];
                if(i!=j&&j!=k&&i!=k&&ch <= k){
                    maxx = max(maxx, ch);
                }
            }
        }
    }

    cout << maxx; 

}
