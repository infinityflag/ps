void solve(){
    cin >> n;
    int minn = 1e9;
    for(int i=0;i<n;i++){
        cin >> a[i];
        minn = min(minn, a[i]);
    }   
    
    for(int i=1;i<=minn;i++){
        if(n%a[i]==0){
            
        }
    }
    /*
