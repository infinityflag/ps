#include <iostream>
#include <algorithm>
#include <iomanip>
#include <queue>
#include <stack>
#include <set>
#include <map>
#include <cstdlib>
#include <cstdio>
#include <vector>
#include <stdlib.h>
#include <cstring>
#include <cmath>
#include <math.h>
#include <string>
typedef long long ll;
using namespace std;
//char a[100];
int main() {
    int n;
    cin >> n;
    map<int,string>m; 
    for(int i=0;i<n;i++){
        int age;
        string name;
        cin >> age >> name;
        m[age] = name;
    }
    sort(m.begin(), m.end();
    for(int x:m){
         cout << x.first << ' '<<x.second <<'\n';
    }

}
